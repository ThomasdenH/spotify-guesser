{
  "name": "spotifyguesser",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "build:deploy": "parcel build index.html --public-url /spotify-guesser/ --no-source-maps",
    "lint": "prettier \"**/*.{ts,tsx}\" && eslint \"**/*.{ts,tsx}\"",
    "lint-fix": "prettier --write \"**/*.{ts,tsx}\" && eslint --fix \"**/*.{ts,tsx}\"",
    "clean": "rimraf dist .cache",
    "deploy": "git branch -f gh-pages && git checkout gh-pages && git reset --hard origin/master && yarn build:deploy && Copy-Item dist/* -Destination . && git add -A . && git commit -a -m 'gh-pages update' && git push origin gh-pages --force && git checkout master"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@types/styled-components": "^4.1.12",
    "babel-polyfill": "^6.26.0",
    "io-ts": "^1.8.2",
    "jsqr": "^1.2.0",
    "peerjs": "^0.3.24",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3",
    "ts": "^0.2.2"
  },
  "devDependencies": {
    "@types/peerjs": "^0.0.30",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/spotify-web-playback-sdk": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/eslint-plugin-tslint": "^1.4.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.4",
    "parcel-bundler": "^1.12.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3333"
  }
}
